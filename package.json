{
  "name": "hello-react",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "10.x"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.16.3"
  },
  "devDependencies": {
    "@material-ui/core": "^1.2.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.6"
  },
  "scripts": {
    "build": "webpack",
    "start": "webpack && node server.js",
    "server": "nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"yarn build -w --mode development\" \"yarn server\""
  },
  "babel": {
    "presets": [
      [
        "env", {
          "targets": {
            "browsers": ["last 2 Chrome versions"]
          }
        }
      ],
      "react",
      "stage-2"
    ]
  },
  "keywords": [
    "node",
    "glitch",
    "express",
    "react"
  ]
}
